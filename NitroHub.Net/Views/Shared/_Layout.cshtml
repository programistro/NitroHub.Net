<script type="text/javascript">
    function calculate(inputNumberId, number, resultElementId) {
        var resultElements = document.querySelectorAll('#' + resultElementId);
        var inputNumber = document.querySelectorAll('#' + inputNumberId)[1].value;

        if ( (inputNumber == NaN) || (inputNumber == 0) ) {
            inputNumber = 1;
        }

        Array.from(resultElements).forEach(resultElement => {
            if (!isNaN(inputNumber)) {
                const result = parseFloat(inputNumber) * number;
                resultElement.textContent = `${result.toLocaleString()} РУБ / ${inputNumber} шт.`;
            } else {
                resultElement.textContent = "Неверное число";
            }
        } )
    };

    function minus(inputNumberId, number, resultElementId) {
        var resultElements = document.querySelectorAll('#' + resultElementId);
        var inputNumber = document.querySelectorAll('#' + inputNumberId)[1].value;

        console.log(inputNumber)


        if ( (inputNumber == NaN) || (inputNumber == 0) || (inputNumber == -1) || (inputNumber == 1) || (inputNumber == "") ) {
            inputNumber = 1;
        } else {
            inputNumber = inputNumber -1;
        }

        Array.from(resultElements).forEach(resultElement => {
            if (!isNaN(inputNumber)) {
                const result = parseFloat(inputNumber) * number;
                resultElement.textContent = `${result.toLocaleString()} РУБ / ${inputNumber} шт.`;
            } else {
                resultElement.textContent = "Неверное число";
            }
